{
    "name": "2do/contao-zomedia-bundle",
    "type": "symfony-bundle",
    "description": "Contao 4 Zomedia Bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Stephan Pre√ül",
            "homepage": "https://www.prestep.at"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "^4.4",
        "symfony/framework-bundle": "^3.3"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.6"
    },
    "extra": {
        "branch-alias": {
            "dev-4.4": "4.4.x-dev"
        },
        "contao-manager-plugin": "IIDO\\ZomediaBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "IIDO\\ZomediaBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ]
    }
}
